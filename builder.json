{
  "schema": {
    "vendor": {
      "type": "string",
      "required": true,
      "label": "Vendor name",
      "default": "websemantics"
    },
    "slug": {
      "type": "string",
      "required": true,
      "label": "Module slug",
      "default": "generic"
    },
    "description": {
      "type": "string",
      "required": false,
      "label": "Module description",
      "default": "Builder default module template"
    },
    "icon": {
      "type": "string",
      "required": false,
      "label": "Module icon",
      "default": "fa fa-square"
    },
    "landing_page": {
      "type": "string",
      "required": false,
      "label": "Landing Page",
      "options" : ["yes", "no"],
      "default": "yes"
    },
    "docblock": {
      "type": "string",
      "required": false,
      "label": "DocBlock",
      "default": " *"
    }
  }
}
